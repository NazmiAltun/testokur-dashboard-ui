{
  "port": 8080,
  "headers": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Headers": "*",
    "Access-Control-Allow-Methods": "*"
  },
  "routes": {
    "/api/v1/license-types": {
      "options": 204,
      "get": "license-types.json"
    },
    "/api/v1/cities": {
      "options": 204,
      "get": "cities.json"
    },
    "/api/v1/sms/add-credits": {
      "options": 204,
      "post": "wait-for-2sec.js"
    },
    "/api/v1/users/activate": {
      "options": 204,
      "post": "wait-for-2sec.js"
    },
    "/api/v1/users/extend": {
      "options": 204,
      "post": "wait-for-2sec.js"
    },
    "/api/v1/sms/send-admin": {
      "options": 204,
      "post": "wait-for-2sec.js"
    },
    "/api/v1/users": {
      "options": 204,
      "get": "users.js"
    },
    "/api/v1/users/online": {
      "options": 204,
      "get": "online-users.js"
    },
    "/api/v1/users/update-by-admin": {
      "options": 204,
      "post": "wait-for-2sec.js"
    },
    "/api/v1/users/:id": {
      "options": 204,
      "delete": "wait-for-2sec.js",
      "get": "single-user.js"
    },
    "/api/v1/emails": {
      "options": 204,
      "get": "emails.js"
    },
    "/api/v1/report-requests": {
      "options": 204,
      "get": "report-requests.js"
    },
    "/api/v1/sms/today": {
      "options": 204,
      "get": "sms.js"
    },
    "/api/v1/sms/:id/logs": {
      "options": 204,
      "get": "sms-logs.js"
    },
    "/api/v1/statistics": {
      "options": 204,
      "get": "statistics.js"
    }
  }
}
