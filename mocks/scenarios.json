{
  "port": 8080,
  "headers": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Headers": "*",
    "Access-Control-Allow-Methods": "*"
  },
  "routes": {
    "/api/v1/license-types": {
      "options": 204,
      "get": "license-types.json"
    },
    "/api/v1/cities": {
      "options": 204,
      "get": "cities.json"
    },
    "/api/v1/sms/add-credits": {
      "options": 204,
      "post": 204
    },
    "/api/v1/users/activate": {
      "options": 204,
      "post": 204
    },
    "/api/v1/sms/send-admin": {
      "options": 204,
      "post": 204
    },
    "/api/v1/users": {
      "options": 204,
      "get": "users-api.js"
    },
    "/api/v1/users/update-by-admin": {
      "options": 204,
      "post": "update-user.js"
    },
    "/account/users": {
      "options": 204,
      "get": "users.json"
    },
    "/account/reset-user-password-by-admin": {
      "options": 204,
      "post": 204
    },
    "/api/v1/users/:id": {
      "options": 204,
      "delete": 204
    }
  }
}
